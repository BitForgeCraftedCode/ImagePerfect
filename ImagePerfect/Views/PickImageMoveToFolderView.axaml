<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 xmlns:vm="using:ImagePerfect.ViewModels"
			 x:DataType="vm:PickImageMoveToFolderViewModel"
             x:Class="ImagePerfect.Views.PickImageMoveToFolderView">
	<!--Note: Tag is for SelectedImages see VM and MainWindow xml-->
	<Button Margin="5" HorizontalAlignment="Center"
		Command="{Binding SelectMoveImagesToFolderCommand}"  
		CommandParameter="{Binding Tag, RelativeSource={RelativeSource AncestorType=UserControl}}"
		Classes="menu-btn">
		Move Selected
	</Button>
</UserControl>
